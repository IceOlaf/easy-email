{
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=31536000"
        }
      ]
    }
  ]
}
